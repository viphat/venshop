<% unless f.empty? %>
  <% if f.first == 'event' %>
    <%= raw f.second %>
  <% else %>
    <div class="alert alert-<%= f.first.in?(['notice','success']) ? 'success' : 'danger' %>">
      <a href="#" data-dismiss="alert" class="close">Ã—</a>
      <ul>
        <li><%= f.second %></li>
      </ul>
    </div>
  <% end %>
<% end %>
